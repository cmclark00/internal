# wpa_supplicant configuration for muOS with rtl8188eu support
# This configuration enables WEXT driver support needed for rtl8188eu adapter

# Core configuration
CONFIG_DRIVER_NL80211=y
CONFIG_DRIVER_WEXT=y
CONFIG_LIBNL32=y

# IEEE 802.11 protocols
CONFIG_IEEE80211N=y
CONFIG_IEEE80211AC=y
CONFIG_IEEE80211AX=y
CONFIG_IEEE80211W=y

# WPA/WPA2/WPA3 support
CONFIG_WPA3_SAE=y
CONFIG_SAE=y
CONFIG_SUITEB=y
CONFIG_SUITEB192=y

# EAP support
CONFIG_EAP=y
CONFIG_EAP_MD5=y
CONFIG_EAP_TLS=y
CONFIG_EAP_MSCHAPV2=y
CONFIG_EAP_PEAP=y
CONFIG_EAP_TTLS=y
CONFIG_EAP_GTC=y
CONFIG_EAP_OTP=y
CONFIG_EAP_LEAP=y
CONFIG_EAP_PSK=y
CONFIG_EAP_PAX=y
CONFIG_EAP_SAKE=y
CONFIG_EAP_GPSK=y
CONFIG_EAP_GPSK_SHA256=y
CONFIG_EAP_PWD=y
CONFIG_EAP_EKE=y
CONFIG_EAP_IKEV2=y
CONFIG_EAP_TNC=y

# PKCS#12 (PFX) support
CONFIG_PKCS12=y

# SmartCard support
CONFIG_SMARTCARD=y

# Wi-Fi Protected Setup (WPS)
CONFIG_WPS=y

# Control interface
CONFIG_CTRL_IFACE=y
CONFIG_CTRL_IFACE_UNIX=y

# Backend for configuration files
CONFIG_BACKEND=file

# Remove deprecated networks
CONFIG_REMOVE_UNUSED_SCAN_RESULTS=y

# P2P support
CONFIG_P2P=y
CONFIG_WIFI_DISPLAY=y

# Support for external programs (ap_scan=0)
CONFIG_AP=y

# IEEE 802.11r (Fast BSS Transition)
CONFIG_IEEE80211R=y

# IEEE 802.11k (RRM)
CONFIG_IEEE80211K=y

# Hotspot 2.0
CONFIG_HS20=y
CONFIG_INTERWORKING=y

# Support for matching SSIDs in scan results
CONFIG_MATCH_IFACE=y

# Internal TLS implementation
CONFIG_TLS=internal

# SHA256-based stronger algorithms
CONFIG_TLSV12=y

# Debugging options (can be disabled for production)
CONFIG_DEBUG_FILE=y
CONFIG_DEBUG_SYSLOG=y

# Privilege separation
CONFIG_PRIVSEP=y

# D-Bus support
CONFIG_CTRL_IFACE_DBUS_NEW=y
CONFIG_CTRL_IFACE_DBUS_INTRO=y

# Automatic channel selection
CONFIG_ACS=y

# Mesh networking
CONFIG_MESH=y

# DPP (Device Provisioning Protocol)
CONFIG_DPP=y
CONFIG_DPP2=y

# OWE (Opportunistic Wireless Encryption)
CONFIG_OWE=y

# FILS (Fast Initial Link Setup)
CONFIG_FILS=y
CONFIG_FILS_SK_PFS=y

# MBO (Multiband Operation)
CONFIG_MBO=y

# WNM (Wireless Network Management)
CONFIG_WNM=y
